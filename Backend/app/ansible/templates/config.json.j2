{
    "deployment_id": "{{ deployment_id }}",
    "deployment_name": "{{ deployment_name }}",
    "framework": "{{ framework }}",
    "model_path": "{{ model_local_path | default(model_repo_id) }}",
    "model_repo_id": "{{ model_repo_id }}",
    "model_revision": "{{ model_revision | default('main') }}",
    "host": "{{ host | default('0.0.0.0') }}",
    "port": {{ port }},
    "served_model_name": "{{ served_model_name | default(model_repo_id.split('/')[-1]) }}",
    "tensor_parallel_size": {{ tensor_parallel_size | default(1) }},
    "gpu_memory_utilization": {{ gpu_memory_utilization | default(0.9) }},
    "gpu_devices": {{ gpu_devices | default([]) | to_json }},
    "env": {{ env_table | default([]) | to_json }},
    "args": {{ args_table | default([]) | to_json }},
    "log_dir": "{{ log_root }}/{{ deployment_id }}",
    "work_dir": "{{ work_dir }}/{{ deployment_id }}"
}
